<idea-plugin>
    <id>MortalisTheme</id>
    <name>Mortalis Theme</name>
    <version>1.1</version>
    <category>UI</category>

    <idea-version since-build="213" until-build="223.*"/>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <themeProvider id="MortalisTheme" path="/MortalisTheme.theme.json"/>
    </extensions>
</idea-plugin>